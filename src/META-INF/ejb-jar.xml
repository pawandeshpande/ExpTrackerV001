<?xml version = '1.0' encoding = 'windows-1252'?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/j2ee/ejb-jar_3_0.xsd"
         version="3.0" xmlns="http://java.sun.com/xml/ns/javaee">
  <enterprise-beans>    
    <session>
      <ejb-name>SessionEJB</ejb-name>
      <home>com.expensetracker.bean.SessionEJBLocal</home>
      <remote>com.expensetracker.bean.SessionEJB</remote>
      <ejb-class>com.expensetracker.bean.SessionEJBBean</ejb-class>
      <session-type>Stateless</session-type>      
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>
</ejb-jar>


